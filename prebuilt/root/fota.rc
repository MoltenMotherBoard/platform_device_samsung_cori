on early-init
    start ueventd

on init
    export PATH /sbin:/vendor/bin:/system/sbin:/system/bin:/system/xbin
    export LD_LIBRARY_PATH /vendor/lib:/system/lib
    export ANDROID_ROOT /system
    export ANDROID_DATA /data
    export EXTERNAL_STORAGE /sdcard

    symlink /system/etc /etc

    mkdir /sdcard
    mkdir /system
    mkdir /data
    mkdir /cache
    mkdir /tmp
    mkdir /dbdata
    mkdir /mnt 0775 root root
    mkdir /mnt/.lfs 0775 root root
    #mount /tmp /tmp tmpfs   

on early-fs

    # rfs drivers
    insmod fsr.ko
    insmod fsr_stl.ko
    insmod rfs_glue.ko
    insmod rfs_fat.ko

on fs
    mount tmpfs tmpfs /tmp
    mount ext4 /dev/block/mmcblk0p7 /system
    mount ext4 /dev/block/mmcblk0p7 /system remount
   	exec check_filesystem /dev/block/mmcblk0p8 ext4
    mount ext4 /dev/block/mmcblk0p8 /cache nosuid nodev

	#J4FS module
    insmod j4fs.ko

    mount j4fs /dev/block/mmcblk0p4 /mnt/.lfs 

	exec check_filesystem /dev/block/mmcblk0p9 ext4
    mount ext4 /dev/block/mmcblk0p9 /data nosuid nodev

    insmod sec_param.ko

	chown system system /data
    chmod 0771 /data  


    
on post-fs
    write /proc/sys/kernel/panic_on_oops 1
    write /proc/sys/kernel/hung_task_timeout_secs 0
    write /proc/cpu/alignment 4
    write /proc/sys/kernel/sched_latency_ns 10000000
    write /proc/sys/kernel/sched_wakeup_granularity_ns 2000000
    write /proc/sys/kernel/sched_compat_yield 1
    write /proc/sys/kernel/sched_child_runs_first 0

    chown system cache /cache
    chmod 0770 /cache
    
    mkdir /data/cache
    chown system cache /data/cache
    chmod 0770 /data/cache

    chown system cache /cache/recovery
    chmod 0770 /cache/recovery
on boot

    ifup lo
    hostname localhost
    domainname localdomain

    class_start default

service ueventd /sbin/ueventd
    critical

service ua_launcher /sbin/ua_launcher
    oneshot
